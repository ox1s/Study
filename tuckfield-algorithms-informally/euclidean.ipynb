   ],
   "source": [
    "def gcd(x,y):\n",
    "    larger = max(x,y)\n",
    "    smaller = min(x,y)\n",
    "\n",
    "    remainder = larger % smaller\n",
    "\n",
    "    if(remainder == 0):\n",
    "        return(smaller)\n",
    "\n",
    "    if(remainder != 0):\n",
    "        return(gcd(smaller,remainder))\n",
    "\n",
    "print(f\"gcd(544,19) = {gcd(544,19)}\") "
   ]
  }
